<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>first</name>
  <description />
  <settings>
    <options>
      <option name="artifactRules" value="output/**" />
    </options>
    <disabled-settings>
      <setting-ref ref="test" />
    </disabled-settings>
    <parameters />
    <build-runners>
      <runner id="nodejs_runner" name="install" type="nodejs-runner">
        <parameters>
          <param name="plugin.docker.imageId" value="node:lts" />
          <param name="plugin.docker.imagePlatform" value="linux" />
          <param name="shellScript" value="npm ci" />
          <param name="teamcity.kubernetes.executor.pull.policy" value="" />
